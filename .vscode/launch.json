    "version": "0.2.0",
    "configurations": [
        {
            "name": "自定义转换器",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "${workspaceFolder}/index.mjs"
            ],
            "sourceMaps": true,
            "stopOnEntry": false,
            "type": "node"
        },
        {
            "name": "调试 ts 源码",
            "program": "${workspaceFolder}/TypeScript/built/local/tsc.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "${workspaceFolder}/test.ts"
            ],
            "sourceMaps": true,
            "stopOnEntry": true,
            "type": "node"
        }
    ]
}